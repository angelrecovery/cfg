{
  "telemetry": {
    "metrics": true,
    "diagnostics": true
  },
  "theme": {
    "mode": "light",
    "light": "Ayu Light",
    "dark": "Ayu Mirage"
  },
  "auto_install_extensions": {
    "toml": true,
    "html": true,
    "env": true,
    "zig": true,
    "lua": true,
    "dockerfile": true
  },
  "calls": {
    "mute_on_join": true
  },

  "restore_on_startup": "none",

  "base_keymap": "VSCode",
  "vim_mode": true,

  "middle_click_paste": false,

  "ui_font_size": 15.5,
  "buffer_font_size": 15.0
}
