{
  "telemetry": {
    "metrics": true,
    "diagnostics": true
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "assistant": {
    "enabled": true,
    "default_width": 480,
    "version": "2",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "editor_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    }
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "toml": true,
    "html": true,
    "svelte": true,
    "env": true,
    "zig": true,
    "lua": true,
    "sql": true,
    "ini": true,
    "make": true,
    "fish": true,
    "swift": true,
    "gitignore": true,
    "dockerfile": true
  },
  "languages": {
    "Zig": {
      "format_on_save": "off"
    }
  },
  "calls": {
    "mute_on_join": true
  },
  "vim": {
    "default_mode": "helix_normal"
  },
  "vim_mode": true,
  "base_keymap": "VSCode",
  "restore_on_startup": "none",
  "middle_click_paste": false,
  "ui_font_size": 15.5,
  "buffer_font_size": 15.0
}
